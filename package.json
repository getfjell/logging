{
  "name": "@fjell/logging",
  "version": "4.4.7",
  "description": "Logging for Fjell",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "logging",
    "logger",
    "log",
    "fjell"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "lint": "eslint . --ext .ts --fix",
    "dev": "concurrently \"tsc --noEmit --watch\" \"vite build --watch\"",
    "build": "pnpm run lint && tsc --noEmit && vite build",
    "clean": "rimraf dist",
    "test": "pnpm run lint && vitest run --coverage",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "kodrdriv:link": "kodrdriv link",
    "kodrdriv:unlink": "kodrdriv unlink",
    "kodrdriv:commit": "kodrdriv commit",
    "kodrdriv:publish": "kodrdriv publish"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@swc/core": "^1.12.14",
    "@tsconfig/recommended": "^1.0.10",
    "@types/node": "^24.0.13",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.0",
    "eslint": "^9.31.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-node": "^7.0.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.13.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getfjell/logging.git"
  }
}
